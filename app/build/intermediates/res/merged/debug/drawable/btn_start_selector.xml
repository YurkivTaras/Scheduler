<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@color/btn_start_normal" />
            <stroke android:width="@dimen/btn_stroke_width" android:color="@color/btn_start_stroke" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/btn_start_pressed" />
            <stroke android:width="@dimen/btn_stroke_width" android:color="@color/btn_start_stroke" />
        </shape>
    </item>
</selector>